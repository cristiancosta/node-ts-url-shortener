{
  "name": "node-ts-url-shortener",
  "version": "1.0.0",
  "description": "Node ts url shortener",
  "scripts": {
    "start": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "build": "tsc",
    "serve": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristiancosta/node-ts-url-shortener.git"
  },
  "keywords": [
    "node",
    "ts",
    "url",
    "shortener"
  ],
  "author": "Cristian Costa <cristiancosta1991@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristiancosta/node-ts-shortener/issues"
  },
  "homepage": "https://github.com/cristiancosta/node-ts-shortener#readme",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint .",
      "prettier --write ."
    ]
  }
}
